{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/api/*",
      "methods": ["POST"],
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.{css,scss,gif,ico,jpg,jpeg,js,png,svg,webp,woff,woff2}"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
